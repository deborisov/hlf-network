{
	"name": "chaincode",
	"description": "My first exciting chaincode implemented in node.js",
	"engines": {
		"node": ">=12",
		"npm": ">=6.9"
	},
	"scripts": {
		"start": "fabric-chaincode-node start",
		"test": "nyc mocha --recursive"
	},
	"engine-strict": true,
	"engineStrict": true,
	"version": "1.0.0",
	"main": "index.js",
	"author": "",
	"license": "Apache-2.0",
	"dependencies": {
		"fabric-contract-api": "^2.0.0",
		"fabric-shim": "^2.0.0"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^6.8.0",
		"mocha": "^7.1.1",
		"nyc": "^15.0.0",
		"sinon": "^9.0.1",
		"sinon-chai": "^3.5.0",
		"winston": "^3.2.1"
	},
	"nyc": {
		"exclude": [
			"coverage/**",
			"test/**"
		],
		"reporter": [
			"text-summary",
			"html"
		],
		"all": true,
		"check-coverage": true,
		"statements": 100,
		"branches": 100,
		"functions": 100,
		"lines": 100
	}
}
